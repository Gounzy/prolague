p_start(IV2,IV3,IV4,R) :- {OV0 = 0, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p2(OV0,OV1,OV2,OV3,OV4,R).

% if V2 < V3, goto p7
% else goto p3
p2(IV0,IV1,IV2,IV3,IV4,R) :- {IV2 < IV3, OV0 = IV0, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p7(OV0,OV1,OV2,OV3,OV4,R).
p2(IV0,IV1,IV2,IV3,IV4,R) :- {IV2 >= IV3, OV0 = IV0, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p3(OV0,OV1,OV2,OV3,OV4,R).

% if V2 < V4, goto p7
% else goto p5
p3(IV0,IV1,IV2,IV3,IV4,R) :- {IV2 < IV4, OV0 = IV0, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p7(OV0,OV1,OV2,OV3,OV4,R).
p3(IV0,IV1,IV2,IV3,IV4,R) :- {IV2 >= IV4, OV0 = IV0, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p5(OV0,OV1,OV2,OV3,OV4,R).

% (V2 >= V4 et V2 >= V3)
% return OVO (= V2)
p5(IV0,IV1,IV2,IV3,IV4,R) :- {OV0 = IV2, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p6(OV0,OV1,OV2,OV3,OV4,R).
p6(IV0,IV1,IV2,IV3,IV4,IV0) :- {}.
p7(IV0,IV1,IV2,IV3,IV4,R) :- {IV3 < IV2, OV0 = IV0, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p13(OV0,OV1,OV2,OV3,OV4,R).
p7(IV0,IV1,IV2,IV3,IV4,R) :- {IV3 >= IV2, OV0 = IV0, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p9(OV0,OV1,OV2,OV3,OV4,R).
p9(IV0,IV1,IV2,IV3,IV4,R) :- {IV3 < IV4, OV0 = IV0, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p13(OV0,OV1,OV2,OV3,OV4,R).
p9(IV0,IV1,IV2,IV3,IV4,R) :- {IV3 >= IV4, OV0 = IV0, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p11(OV0,OV1,OV2,OV3,OV4,R).
p11(IV0,IV1,IV2,IV3,IV4,R) :- {OV0 = IV3, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p12(OV0,OV1,OV2,OV3,OV4,R).
p12(IV0,IV1,IV2,IV3,IV4,R) :- {OV0 = IV0, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p6(OV0,OV1,OV2,OV3,OV4,R).
p13(IV0,IV1,IV2,IV3,IV4,R) :- {OV0 = IV4, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p14(OV0,OV1,OV2,OV3,OV4,R).
p14(IV0,IV1,IV2,IV3,IV4,R) :- {OV0 = IV0, OV1 = IV1, OV2 = IV2, OV3 = IV3, OV4 = IV4}, p6(OV0,OV1,OV2,OV3,OV4,R).
